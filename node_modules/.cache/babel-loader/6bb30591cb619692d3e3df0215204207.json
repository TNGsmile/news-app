 // payload： 负荷，约定是一个数组\n    setData(state, payload) {\n      state.data = payload;\n    },\n  },\n  actions: {\n    async fetchDatas(context) {\n      // 设置isLoading为true\n      context.commit(\"setIsLoading\", true);\n      var channels = await getNewsChannels();\n      // 设置data为channels\n      context.commit(\"setData\", channels);\n      // 设置isLoading为false\n      context.commit(\"setIsLoading\", false);\n    },\n  },\n};\n"]}]}