,KAAA,CAAA,aAAA,CAAA,UAAA,EAAA,OAAA,CAJA;AAAA;AAAA;AAAA;;AAAA;AAAA,uBAOA,GAAA,CAAA,KAAA,CAAA,QAAA,CAPA;;AAAA;AAOA,gBAAA,IAPA;;AAQA,oBAAA,IAAA,CAAA,IAAA,EAAA;AACA;AACA,kBAAA,KAAA,CAAA,eAAA,CAAA;;AACA,kBAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AACA,oBAAA,IAAA,EAAA;AADA,mBAAA;AAGA,iBANA,MAOA;AACA,kBAAA,KAAA,CAAA,KAAA,CAAA,KAAA,GAAA,IAAA,CAAA,GAAA;AACA;;AAjBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAmBA,KA9BA;AA+BA,IAAA,gBA/BA,8BA+BA;AACA,UAAA,MAAA,GAAA,KAAA,aAAA,CAAA,UAAA,EAAA,UAAA,CAAA;;AACA,UAAA,MAAA,EAAA;AACA,YAAA,KAAA,QAAA,CAAA,QAAA,KAAA,KAAA,QAAA,CAAA,QAAA,EAAA;AACA,eAAA,KAAA,CAAA,QAAA,GAAA,SAAA;AACA,iBAAA,KAAA;AACA;;AACA,eAAA,IAAA;AACA;;AACA,aAAA,KAAA;AACA;AAzCA;AAtBA,CAAA","sourcesContent":["<template>\n  <Center>\n    <form class=\"login-container\"\n          @submit.prevent=\"handelSubmit\">\n      <div class=\"form-item\">\n        <div class=\"input\">\n          <label for=\"\">账号:</label><input v-model=\"userInfo.loginId\"\n                 type=\"text\"\n                 @input=\"validateEmpty('loginId','账号不能为空')\" /></div>\n        <FormError :msg=\"error.loginId\" />\n      </div>\n      <div class=\"form-item\">\n        <div class=\"input\"><label for=\"\">密码:</label><input v-model=\"userInfo.loginPwd\"\n                 type=\"password\"\n                 autocomplete=\"new-password\"\n                 @input=\"validateEmpty('loginPwd','密码不能为空')\" /></div>\n        <FormError :msg=\"error.loginPwd\" />\n      </div>\n      <div class=\"form-item\">\n        <div class=\"input\"><label for=\"\">重复密码:</label><input v-model=\"userInfo.pwdAgain\"\n                 type=\"password\"\n                 autocomplete=\"new-password\"\n                 @input=\"validatePwdAgain\" /></div>\n        <FormError :msg=\"error.pwdAgain\" />\n      </div>\n      <div class=\"form-item\">\n        <div class=\"input\"><label for=\"\">昵称:</label><input v-model=\"userInfo.nickname\"\n                 type=\"text\"\n                 autocomplete=\"new-password\"\n                 @input=\"validateEmpty('nickname','请输入昵称')\" /></div>\n        <FormError :msg=\"error.nickname\" />\n      </div>\n      <div class=\"form-item\">\n        <div class=\"input\"><label for=\"\"></label>\n          <button>注册</button>\n        </div>\n        <FormError :isCenter=\"true\"\n                   :msg=\"error.sever\" />\n      </div>\n    </form>\n  </Center>\n</template>\n\n<script>\nimport Center from \"../components/Center\";\nimport FormError from \"../components/FormError\";\nimport { reg } from \"../services/userService\"\nexport default {\n  components: {\n    Center,\n    FormError\n  },\n  data () {\n    return {\n      userInfo: {\n        loginId: \"\",\n        loginPwd: \"\",\n        pwdAgain: \"\",\n        nickname: \"\",\n      },\n      error: {\n        loginId: \"\",\n        loginPwd: \"\",\n        pwdAgain: \"\",\n        nickname: \"\",\n        sever: \"\",\n      }\n    }\n  },\n  methods: {\n    // 非空验证\n    validateEmpty (field, msg) {\n      if (this.userInfo[field]) {\n        this.error[field] = \"\";\n        return true\n      } else {\n        this.error[field] = msg;\n        return false\n      }\n    },\n    async handelSubmit () {\n      if (this.validateEmpty('loginId', '账号不能为空') &\n        this.validateEmpty('loginPwd', '密码不能为空') &\n        this.validatePwdAgain() &\n        this.validateEmpty('nickname', '请输入昵称')\n      ) {\n        //  提交数据\n        var resp = await reg(this.userInfo)\n        if (resp.data) {\n          // 注册成功\n          alert(\"注册成功!!!!!!!!!\");\n          this.$router.push({\n            name: \"Login\",\n          })\n        }\n        else {\n          this.error.sever = resp.msg;\n        }\n      }\n    },\n    validatePwdAgain () {\n      var result = this.validateEmpty(\"pwdAgain\", \"重复密码不能为空\")\n      if (result) {\n        if (this.userInfo.loginPwd !== this.userInfo.pwdAgain) {\n          this.error.pwdAgain = \"两次密码不一致\"\n          return false;\n        }\n        return true;\n      }\n      return false;\n    }\n  }\n};\n</script> \n\n<style scoped>\n.login-container {\n  width: 400px;\n}\n\n.form-item .input {\n  display: flex;\n}\n.form-item label {\n  width: 100px;\n  display: inline-block;\n  text-align: right;\n  line-height: 41px;\n}\n\n.form-item input,\n.form-item button {\n  flex-grow: 1;\n}\n</style>"],"sourceRoot":"src/views"}]}